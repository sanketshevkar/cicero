{
  "name": "@accordproject/cicero-server",
  "version": "0.22.2",
  "description": "Cicero Server - wraps the Cicero Engine and exposes it as a RESTful service",
  "engines": {
    "node": ">=12",
    "npm": ">=5"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "cicero-server": "./app.js"
  },
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check",
    "test": "mocha",
    "test:windows": "echo 'No Windows tests for this package'",
    "test:cov": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/accordproject/cicero.git",
    "directory": "packages/cicero-server"
  },
  "keywords": [
    "accord",
    "legal",
    "tech",
    "smart",
    "contract"
  ],
  "author": "clause.io",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/accordproject/cicero/issues"
  },
  "homepage": "https://github.com/accordproject/cicero#readme",
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-things": "0.2.0",
    "decache": "4.4.0",
    "eslint": "8.1.0",
    "jsdoc": "^3.6.7",
    "license-check": "1.1.5",
    "mocha": "6.1.4",
    "mockery": "2.0.0",
    "nyc": "14.1.1",
    "sinon": "4.0.1",
    "supertest": "3.0.0"
  },
  "dependencies": {
    "@accordproject/cicero-core": "0.22.2",
    "@accordproject/cicero-engine": "0.22.2",
    "@accordproject/cicero-transform": "0.22.2-20210714131804",
    "express": "4.16.2"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./app.js",
      "!./test/data/**/*",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "nyc": {
    "produce-source-map": "true",
    "sourceMap": "inline",
    "reporter": [
      "lcov",
      "text-summary",
      "html",
      "json"
    ],
    "include": [
      "app.js"
    ],
    "exclude": [],
    "all": true,
    "check-coverage": true,
    "statements": 93,
    "branches": 70,
    "functions": 92,
    "lines": 93
  },
  "gitHead": "514fd02f46752e018aedb77e1716a212ef4a59e5"
}
