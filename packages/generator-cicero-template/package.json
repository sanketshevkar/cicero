{
  "name": "@accordproject/generator-cicero-template",
  "version": "0.22.2",
  "description": "Code generator for a Cicero Template",
  "engines": {
    "node": ">=12",
    "npm": ">=5"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "clause.io",
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check",
    "test": "mocha",
    "test:windows": "echo 'No Windows tests for this package'",
    "test:cov": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/accordproject/cicero.git",
    "directory": "packages/generator-cicero-template"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/accordproject/cicero/issues"
  },
  "homepage": "https://github.com/accordproject/cicero#readme",
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "cicero",
    "template",
    "smart contracts",
    "clause",
    "accord project",
    "yeoman-generator"
  ],
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-things": "0.2.0",
    "eslint": "8.1.0",
    "jsdoc": "^3.6.7",
    "license-check": "1.1.5",
    "mocha": "6.1.4",
    "mocha-lcov-reporter": "1.3.0",
    "mockery": "2.0.0",
    "nyc": "14.1.1",
    "sinon": "4.0.1",
    "yeoman-assert": "3.1.0",
    "yeoman-test": "2.1.0"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./test/data/**/*",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./generatos/app/template/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "dependencies": {
    "chalk": "2.3.2",
    "yeoman-generator": "^3.1.1",
    "yosay": "2.0.1"
  },
  "nyc": {
    "produce-source-map": "true",
    "sourceMap": "inline",
    "reporter": [
      "lcov",
      "text-summary",
      "html",
      "json"
    ],
    "include": [
      "generators/app/**/*.js"
    ],
    "exclude": [
      "generators/app/templates/**"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 86,
    "branches": 64,
    "functions": 88,
    "lines": 85
  },
  "gitHead": "514fd02f46752e018aedb77e1716a212ef4a59e5"
}
